{
    "name": "bulton-fr/bfw",
    "type": "library",
    "description": "Framework PHP",
    "keywords": [
        "framework",
        "php"
    ],
    "homepage": "https://github.com/bulton-fr/bfw",
    "time": "2016-05-29",
    "license": "LGPL-3.0",
    "authors": [
        {
            "name": "Vermeulen Maxime / Bulton",
            "homepage": "http://www.bulton.fr",
            "email": "bulton.fr@gmail.com"
        }
    ],
    "support": {
        "email": "support@bulton.fr",
        "issues": "https://github.com/bulton-fr/bfw/issues",
        "source": "https://github.com/bulton-fr/bfw"
    },
    "require": {
        "php": ">=5.6.0",
        "bulton-fr/dependency-tree": "@stable"
    },
    "require-dev": {
        "atoum/atoum": "~3.1",
        "atoum/visibility-extension": "~1.3"
    },
    "suggest": {
        "ext-memcached": "*",
        "bulton-fr/bfw-controller": "@stable",
        "bulton-fr/bfw-sql": "@stable"
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "BFW\\": "src/class",
            "BFW\\Core\\": "src/class/core",
            "BFW\\Helpers\\": "src/helpers",
            "BFW\\Install\\": "install/class/",
            "BFW\\Memcache\\": "src/class/memcache"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BFW\\Test\\Mock\\": "test/unit/mocks/src/class",
            "BFW\\Test\\Helpers\\": "test/unit/helpers",
            "BFW\\Install\\Test\\Mock\\": "test/unit/mocks/src/class/install",
            "BFW\\Install\\Test\\Helpers\\": "test/unit/helpers/install",
            "BFW\\Core\\Test\\Mock\\": "test/unit/mocks/src/class/core",
            "BFW\\Memcache\\test\\unit\\": "test/unit/src/class/memcache",
            "BFW\\Helpers\\Test\\Mock\\": "test/unit/mocks/src/helpers"
        }
    },
    "bin": [
        "bin/bfwInstall",
        "bin/bfwInstallModules"
    ]
}
