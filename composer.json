{
    "name": "bulton-fr/bfw",
    "type": "library",
    "description": "Framework PHP",
    "keywords": [
        "framework",
        "php"
    ],
    "homepage": "https://github.com/bulton-fr/bfw",
    "time": "2016-05-29",
    "license": "LGPL-3.0",
    "authors": [
        {
            "name": "Vermeulen Maxime / Bulton",
            "homepage": "http://www.bulton.fr",
            "email": "bulton.fr@gmail.com"
        }
    ],
    "support": {
        "email": "support@bulton.fr",
        "issues": "https://github.com/bulton-fr/bfw/issues",
        "source": "https://github.com/bulton-fr/bfw"
    },
    "require": {
        "php": "^7.0.0",
        "bulton-fr/dependency-tree": "@stable",
        "monolog/monolog": "~1.0"
    },
    "require-dev": {
        "atoum/atoum": "~3.1",
        "atoum/visibility-extension": "~1.3"
    },
    "suggest": {
        "ext-memcached": "To save datas into your memory and be faster to access it",
        "bulton-fr/bfw-api": "API module for the framework",
        "bulton-fr/bfw-controller": "Controller module for the framework",
        "bulton-fr/bfw-fastroute": "Router module for the framework who use fastroute lib",
        "bulton-fr/bfw-fenom": "Template module for the framework who use fenom",
        "bulton-fr/bfw-sql": "Queries systems module for the framework"
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "BFW\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BFW\\Test\\Mock\\": "test/unit/mocks/src",
            "BFW\\Test\\Helpers\\": "test/unit/helpers"
        }
    },
    "bin": [
        "bin/bfwInstall",
        "bin/bfwInstallModules"
    ]
}
